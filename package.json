{
  "name": "jest-ld-mock",
  "version": "0.2.3",
  "description": "Easily unit test LaunchDarkly feature flagged components with jest",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "prep-dev": "yarn build && yarn --cwd example install && mkdir -p example/node_modules/jest-ld-mock && cp -r dist example/node_modules/jest-ld-mock && cp package.json example/node_modules/jest-ld-mock",
    "lint": "eslint --ext ts,tsx,js,jsx src",
    "test": "jest",
    "build-publish": "npm run build && npm version patch -m 'Upgrade to %s' && npm publish && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchdarkly-labs/jest-launchdarkly-mock.git"
  },
  "keywords": [
    "jest",
    "launchdarkly",
    "mock",
    "unit",
    "test",
    "feature",
    "flags"
  ],
  "author": "Yusinto Ngadiman",
  "license": "Apache-2.0",
  "homepage": "https://github.com/launchdarkly-labs/jest-launchdarkly-mock#readme",
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "3.4.2",
    "@types/jest": "26.0.14",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.4.2",
    "launchdarkly-js-sdk-common": "^3.2.10",
    "launchdarkly-react-client-sdk": "^2.20.2",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.0",
    "typescript": "^4.0.3"
  }
}
